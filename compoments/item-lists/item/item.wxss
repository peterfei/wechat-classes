.header-img-s {
  width: 95px;
  height: 95px;
  border-radius: 8px;
}
.header {
  display: flex;
  align-items: center;
  border-color: #e9e9e9;
  border-style: solid;
  border-width: 2px 0 2px 0;
  padding: 10px;
  height: 100px;
  flex: 1;
  background-color: white;
}
.header .header-img {
  flex: 2;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.header .header-text {
  flex: 8;
  display: flex;
  flex-direction: column;
  margin: 5px;
}
.text-subtitle {
  display: flex;
  justify-content: space-between;
  color: rgb(110, 110, 110);
}
