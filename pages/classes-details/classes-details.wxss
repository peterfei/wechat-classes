.tabbar {
  position: fixed;
  bottom: 0;
  width: 100%;
  left: 0;
  right: 0;
}
.header-img image {
  width: 105px;
  height: 95px;
  border-radius: 8px;
}
.header {
  display: flex;
  align-items: center;
  border-color: #e9e9e9;
  border-style: solid;
  border-width: 2px 0 2px 0;
  padding: 10px;
}
.header .header-img {
  flex: 2;
}
.header .header-text {
  flex: 4;
  display: flex;
  flex-direction: column;
  margin: 5px;
}
.text-title {
  font-size: 18px;
  color: rgb(75, 75, 75);
}
.text-subtitle {
  display: flex;
  justify-content: space-between;
  color: rgb(110, 110, 110);
}

.category{
    flex:1;
    display:flex;
    justify-content: space-around;
    align-items:center;
    padding: 12px;
}

.category image{
    width:65px;
    height:65px;
}
